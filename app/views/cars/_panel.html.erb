<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title">Машины:</h3>
  </div>
  <div class="panel-body">
    <table class="table table-user-information">
      <thead>
      <tr>
        <th>Название</th>
        <th>Грузоподъмность</th>
        <th>Вместительность</th>
        <th>Тип погрузки</th>
        <th>Тип машины</th>
        <th>Действия</th>
      </tr>
      </thead>
      <tbody>
      <% if current_user.cars.blank? %>
          <h3>Вы не добавили ни одну машину</h3>
      <% else %>
          <%= render current_user.cars %>
      <% end %>
      </tbody>
    </table>
  </div>
</div>